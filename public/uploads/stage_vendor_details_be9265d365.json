{
    "type": "set_value",
    "args": {
        "titleTextStyle": {
            "color": "0xFF000000",
            "letterSpacing": 0.7
        },
        "subTitleTextStyle": {
            "letterSpacing": 1
        },
        "bodyTextTitleStyle": {
            "letterSpacing": 1,
            "fontSize": 16,
            "color": "#2D2D39"
        },
        "buildTitleWidget": {
            "type": "nullable_text",
            "args": {
                "text": "Title",
                "style": "${getTextStyle('context','bodyText1')}"
            }
        },
        "tablist": {
            "values": [
                "General",
                "Nurologist",
                "Orthopedic",
                "Pediatrician"
            ]
        },
        "buildTabListWidget": {
            "type": "container",
            "args": {
                "padding": {
                    "left": 20,
                    "right": 20,
                    "top": 5,
                    "bottom": 5
                },
                "margin": 5,
                "alignment": "center",
                "decoration": {
                    "color": "#D9E4EC",
                    "borderRadius": {
                        "type": "all",
                        "radius": 20
                    }
                }
            },
            "child": {
                "type": "nullable_text",
                "args": {
                    "text": "${value}",
                    "style": "${getTextStyle('context','bodyText2')}"
                }
            }
        }
    },
    "child": {
        "type": "save_context",
        "args": {
            "key": "context"
        },
        "child": {
            "type": "scaffold",
            "args": {
                "backgroundColor": "${getOveriddenColor('context','background',1)}",
               
                "appBar": {
                    "type": "app_bar",
                    "args": {
                        "backgroundColor": "${getOveriddenColor('context','background',1)}",
                        "elevation": 0,
                        "leading": {
                            "type": "icon_button",
                            "args": {
                                "onPressed":"${navigate_pop()}",
                                "icon": {
                                    "type": "icon",
                                    "args": {
                                        "icon": {
                                            "codePoint": "0xe092",
                                            "fontFamily": "MaterialIcons"
                                        },
                                        "color": "#000000",
                                        "size": 25
                                    }
                                }
                            }
                        }
                    }
                },
                "body": {
                    "type": "single_child_scroll_view",
                    "args": {
                        "physics": {
                            "type": "bouncing"
                        }
                    },
                    "child": {
                        "type": "column",
                        "args": {
                            "crossAxisAlignment": "start"
                        },
                        "children": [
                            {
                                "type": "padding",
                                "args": {
                                    "padding": [
                                        20,
                                        20
                                    ]
                                },
                                "child": {
                                    "type": "clip_rrect",
                                    "args": {
                                        "borderRadius": {
                                            "type": "circular",
                                            "radius": 10
                                        }
                                    },
                                    "child": {
                                        "type": "network_image",
                                        "args": {
                                            "height": 100,
                                            "width": 100,
                                            "src": "https://images.unsplash.com/photo-1622253692010-333f2da6031d?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=464&q=80",
                                            "fit": "cover",
                                            "errorBuilder": "${imageErrorBuilder}"
                                        }
                                    }
                                }
                            },
                            {
                                "type": "sized_box",
                                "args": {
                                    "height": 15
                                }
                            },
                            {
                                "type": "container",
                                "args": {
                                    "padding": {
                                        "left": 20,
                                        "right": 20
                                    }
                                },
                                "child": {
                                    "type": "column",
                                    "args": {
                                        "crossAxisAlignment": "start"
                                    },
                                    "children": [
                                        {
                                            "type": "nullable_text",
                                            "args": {
                                                "text": "Dr. Manav Singh",
                                                "style": "${getTextStyle('context','headline4',titleTextStyle)}"
                                            }
                                        },
                                        {
                                            "type": "nullable_text",
                                            "args": {
                                                "text": "Something, something & something",
                                                "style": "${getTextStyle('context','bodyText2',subTitleTextStyle)}"
                                            }
                                        },
                                        {
                                            "type": "sized_box",
                                            "args": {
                                                "height": 15
                                            }
                                        },
                                        "${buildTitleWidget}",
                                        {
                                            "type": "sized_box",
                                            "args": {
                                                "height": 8
                                            }
                                        },
                                        {
                                            "type": "read_more",
                                            "args": {
                                                "text": "Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industrys standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book.",
                                                "style": "${getTextStyle('context','subtitle2')}"
                                            }
                                        },
                                        {
                                            "type": "sized_box",
                                            "args": {
                                                "height": 15
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "container",
                                "args": {
                                    "padding": {
                                        "left": 20
                                    }
                                },
                                "child": "${buildTitleWidget}"
                            },
                            {
                                "type": "sized_box",
                                "args": {
                                    "height": 8
                                }
                            },
                            {
                                "type": "sized_box",
                                "args": {
                                    "height": 45
                                },
                                "child": {
                                    "type": "list_view",
                                    "args": {
                                        "padding": [
                                            10,
                                            0
                                        ],
                                        "physics": {
                                            "type": "bouncing"
                                        },
                                        "scrollDirection": "horizontal"
                                    },
                                    "children": "${for_in(tablist['values'], 'buildTabListWidget')}"
                                }
                            }
                        ]
                    }
                }
            }
        }
    }
}