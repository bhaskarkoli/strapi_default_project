{
  "type": "save_context",
  "args": {
    "key": "context"
  },
  "child": {
    "type": "set_value",
    "args": {
      "buildTabListWidget": {
        "type": "container",
        "args": {
          "padding": {
            "left": 20,
            "right": 20,
            "top": 5,
            "bottom": 5
          },
          "margin": 5,
          "alignment": "center",
          "decoration": {
            "color": "#D9E4EC",
            "borderRadius": {
              "type": "all",
              "radius": 20
            }
          }
        },
        "child": {
          "type": "nullable_text",
          "args": {
            "text": "${value}",
            "style": "${getTextStyle('context','bodyText2')}"
          }
        }
      },
      "tablist": {
        "values": ["General", "Nurologist", "Orthopedic", "Pediatrician"]
      },
      "property_details": {
        "values": [
          {
            "propertyImage": "https://thumbs.dreamstime.com/b/classic-house-flower-garden-751996.jpg",
            "description": "Vakola, Santacruz East",
            "subtitle": "subtitle",
            "title": "₹3.4 Cr",
            "address": "Vakola, Santacruz East",
            "price": "₹3.4 Cr"
          }
        ]
      },
      "errorImage": "assets/error_images/img_image_error.png",
      "imageError": {
        "type": "container",
        "args": {
          "width": 48,
          "height": 48,
          "alignment": "center"
        },
        "child": {
          "type": "asset_image",
          "args": {
            "fit": "contain",
            "name": "${errorImage}"
          }
        }
      },
      "titleTextStyle": {
        "fontSize": 18,
        "fontWeight": "w800",
        "color": "${getColor('#ffffff', 1.0)}"
      },
      "info2TextStyle": {
        "color": "${getColor('#ffffff', 0.75)}",
        "fontSize": 13,
        "fontWeight": "w500"
      },
      "subtitleTextStyle": {
        "color": "#212121",
        "fontSize": 16,
        "fontWeight": "w600"
      },
      "descriptionTextStyle": {
        "color": "#616161",
        "fontSize": 14,
        "fontWeight": "w600"
      },
      "infoTextStyle": {
        "color": "#616161",
        "fontSize": 12,
        "fontWeight": "w600"
      },
      "template": {
        "type": "gesture_detector",
        "args": {
          "onTap": "${navigate_named('/listing','property')}"
        },"child": {
        "type": "card",
        "args": {
          "elevation": 3,
          "margin": [12, 6, 12, 6],
          "shadowColor": "${getColor('#000000', 0.35)}",
          "shape": {
            "type": "rounded",
            "borderRadius": {
              "type": "circular",
              "radius": 16
            }
          }
        },
        "child": {
          "type": "column",
          "args": {
            "mainAxisSize": "min",
            "mainAxisAlignment": "start",
            "crossAxisAlignment": "start"
          },
          "children": [
            {
              "type": "container",
              "args": {
                "height": 144,
                "decoration": {
                  "color": "${getOveriddenColor('context','surfaceVariant',1)}",
                  "borderRadius": {
                    "type": "only",
                    "topLeft": {
                      "type": "circular",
                      "radius": 16
                    },
                    "topRight": {
                      "type": "circular",
                      "radius": 16
                    }
                  }
                }
              },
              "child": {
                "type": "stack",
                "args": {
                  "alignment": "bottomLeft"
                },
                "children": [
                  {
                    "type": "align",
                    "args": {
                      "alignment": "center"
                    },
                    "child": {
                      "type": "clip_rrect",
                      "args": {
                        "borderRadius": {
                          "type": "only",
                          "topLeft": {
                            "type": "circular",
                            "radius": 16
                          },
                          "topRight": {
                            "type": "circular",
                            "radius": 16
                          }
                        },
                        "clipBehavior": "antiAlias"
                      },
                      "child": {
                        "type": "network_image",
                        "args": {
                          "src": "${getImageUrl(value['propertyImage'])}",
                          "fit": "cover",
                          "width": "infinity",
                          "errorBuilder": "${imageErrorBuilder}"
                        }
                      }
                    }
                  },
                  {
                    "type": "container",
                    "args": {
                      "width": "infinity",
                      "padding": [12, 8, 12, 8],
                      "decoration": {
                        "gradient": {
                          "type": "linear",
                          "begin": "bottomCenter",
                          "end": "topCenter",
                          "colors": ["${getColor('#000000', 0.6)}", "#00000000"]
                        }
                      }
                    },
                    "child": {
                      "type": "column",
                      "args": {
                        "mainAxisSize": "min",
                        "mainAxisAlignment": "center",
                        "crossAxisAlignment": "start"
                      },
                      "children": [
                        {
                          "type": "nullable_text",
                          "args": {
                            "text": "${value['title']}",
                            "maxLines": 1,
                            "overflow": "ellipsis",
                            "softWrap": true,
                            "style": "${getTextStyle('context', 'headline6', titleTextStyle)}"
                          }
                        },
                        {
                          "type": "nullable_text",
                          "args": {
                            "text": "${value['description']}",
                            "maxLines": 2,
                            "overflow": "ellipsis",
                            "softWrap": true,
                            "style": "${getTextStyle('context','subtitle2', info2TextStyle)}"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "type": "padding",
              "args": {
                "padding": 12
              },
              "child": {
                "type": "row",
                "args": {
                  "mainAxisSize": "max",
                  "mainAxisAlignment": "start",
                  "crossAxisAlignment": "center"
                },
                "children": [
                  {
                    "type": "expanded",
                    "args": {
                      "flex": 7
                    },
                    "child": {
                      "type": "column",
                      "args": {
                        "mainAxisAlignment": "start",
                        "crossAxisAlignment": "start"
                      },
                      "children": [
                        {
                          "type": "nullable_text",
                          "args": {
                            "text": "${value['subtitle']}",
                            "maxLines": 2,
                            "overflow": "ellipsis",
                            "style": "${getTextStyle('context','subtitle2', subtitleTextStyle)}"
                          }
                        },
                        {
                          "type": "nullable_text",
                          "args": {
                            "text": "${value['description']}",
                            "maxLines": 2,
                            "overflow": "ellipsis",
                            "style": "${getTextStyle('context','bodyText2', descriptionTextStyle)}"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "type": "expanded",
                    "args": {
                      "flex": 3
                    },
                    "child": {
                      "type": "align",
                      "args": {
                        "alignment": "centerRight"
                      },
                      "child": {
                        "type": "nullable_text",
                        "args": {
                          "text": "${value['description']}",
                          "maxLines": 2,
                          "overflow": "ellipsis",
                          "style": "${getTextStyle('context','caption', infoTextStyle)}"
                        }
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    }
    },
    "child": {
      "type": "scaffold",
      "args": {
        "backgroundColor": "${getOveriddenColor('context','background',1)}",
        "floatingActionButton": {
          "type": "floating_action_button",
          "args": {
            "backgroundColor": "${getOveriddenColor('context','primary',1)}"

          },
          "child": {
            "type": "icon",
            "args": {
              "icon": {
                "codePoint": "0xf1ae",
                "fontFamily": "MaterialIcons"
              },
              "size": 25
            }
          }
        },
        "appBar": {
          "type": "app_bar",
          "args": {
            "leading": {
              "type": "icon_button",
              "args": {
                "onPressed": "${navigate_pop()}",
                "icon": {
                  "type": "icon",
                  "args": {
                    "icon": {
                      "codePoint": "0xe092",
                      "fontFamily": "MaterialIcons"
                    },
                    "color": "#000000",
                    "size": 25
                  }
                }
              }
            },
            "backgroundColor": "${getOveriddenColor('context','background',1)}",
            "elevation": 0,
            "title": {
              "type": "row",
              "args": {
                "crossAxisAlignment": "start",
                "mainAxisAlignment": "start",
                "mainAxisSize": "min"
              },
              "children": [
                {
                  "type": "text",
                  "args": {
                    "text": "List"
                  }
                }
              ]
            }
          }
        },
        "body": {
          "type": "column",

          "children": [
            {
              "type": "sized_box",
              "args": {
                "height": 45
              },
              "child": {
                "type": "list_view",
                "args": {
                  "padding": [10, 0],
                  "physics": {
                    "type": "bouncing"
                  },
                  "scrollDirection": "horizontal"
                },
                "children": "${for_each(tablist['values'], 'buildTabListWidget')}"
              }
            },
            {
              "type": "expanded",
              "child": {
                "type": "container",
                "child": {
                  "type": "list_view",
                  "id": "data",
                  "listen": ["data"],
                  "args": {
                    "physics": {
                      "type": "bouncing"
                    },
                    "padding": [0, 12, 0, 12]
                  },
                  "children": "${for_in(property_details['values'], 'template')}"
                }
              }
            }
          ]
        }
      }
    }
  }
}
