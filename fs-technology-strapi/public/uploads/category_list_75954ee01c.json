{
    "type": "set_value",
    "args": {
        "errorImage": "assets/images/img_image_error.png",
        "entries": {
            "values": [
                {
                    "name": "Doctor",
                    "img": "https://images.unsplash.com/photo-1532938911079-1b06ac7ceec7?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1332"
                },
                {
                    "name": "Architect",
                    "img":"https://images.unsplash.com/photo-1503387837-b154d5074bd2?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1331"
                },
                {
                    "name": "Lawyer",
                    "img": "https://images.unsplash.com/photo-1589578527966-fdac0f44566c?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=387"
                },
                {
                    "name": "Chartered Accountant",
                    "img": "https://images.unsplash.com/photo-1626266061368-46a8f578ddd6?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170"
                },
                {
                    "name": "Company Secretary",
                    "img": "https://images.unsplash.com/photo-1517245386807-bb43f82c33c4?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170"
                },
                {
                    "name": "Vendors",
                    "img": "https://images.unsplash.com/photo-1528698827591-e19ccd7bc23d?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1176"
                },
                {
                    "name": "Property",
                    "img": "https://images.unsplash.com/photo-1560520653-9e0e4c89eb11?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1073"
                },
                {
                    "name": "Business Listing",
                    "img": "https://images.unsplash.com/photo-1638913972776-873fc7af3fdf?crop=entropy&cs=tinysrgb&fm=jpg&ixlib=rb-1.2.1&q=80&raw_url=true&ixid=MnwxMjA3fDF8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170"
                }
            ]
        },
        "listTemplate": {
            "type": "list_tile",
            "args": {
                "title": {
                    "type": "text",
                    "args": {
                        "text": "${value['name']}",
                        "style": {
                            "color": "#fff",
                            "fontSize": 30
                        }
                    }
                }
            }
        },
        "template": {
            "type": "padding",
            "args": {
                "padding": 5.0
            },
            "child":{
                "type": "gesture_detector",
                "args": {
                  "onTap": "${navigate_named('/listing',value['name'])}"
                },"child":  {
                "type": "material",
                "args": {
                    "elevation": 0
                },
                "child": {
                    "type": "container",
                    "args": {
                        "decoration": {
                            "color": "#FFFFFF                            ",
                            "borderRadius": {
                                "type": "all",
                                "radius": 10
                            }
                        }
                    },
                    "child": {
                        "type": "stack",
                        "args": {
                            "alignment": "bottomLeft"
                        },
                        "children": [
                            {
                                "type": "align",
                                "args": {
                                    "alignment": "center"
                                },
                                "child": {
                                    "type": "clip_rrect",
                                    "args": {
                                        "borderRadius": {
                                            "type": "all",
                                            "radius": 10
                                        },
                                        "clipBehavior": "antiAlias"
                                    },
                                    "child": {
                                        "type": "network_image",
                                        "args": {
                                            "src": "${value['img']}",
                                            "fit": "cover",
                                            "width": "infinity",
                                            "errorBuilder": "${imageErrorBuilder}"
                                        }
                                    }
                                }
                            },
                            {
                                "type": "opacity",
                                "args": {
                                    "opacity": 0.25
                                },
                                "child": {
                                    "type": "placeholder"
                                }
                            },
                            {
                                "type": "padding",
                                "args": {
                                    "padding": {
                                        "left": 15,
                                        "bottom": 15
                                    }
                                },
                                "child": {
                                    "type": "text",
                                    "args": {
                                        "text": "${value['name']}",
                                        "textScaleFactor": 1,
                                        "style": "${getTextStyle('context','headline4',{'fontWeight':'bold','fontSize':30,'color':'#FFFFFF'})}"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        }
        }
    },
    "child":{
        "type":"save_context",
        "args":{
            "key":"context"
        },
        "child": {
        "type": "scaffold",
        "args": {
            "backgroundColor": "${getOveriddenColor('context','background',1)}",

            "appBar": {
                "type": "app_bar",
                "args": {
                    "leading": {
                        "type": "icon_button",
                        "args": {
                          "onPressed": "${navigate_pop()}",
                          "icon": {
                            "type": "icon",
                            "args": {
                              "icon": {
                                "codePoint": "0xe092",
                                "fontFamily": "MaterialIcons"
                              },
                              "color": "#000000",
                              "size": 25
                            }
                          }
                        }
                      },
                    "elevation": 0,
                    "backgroundColor": "#FFFFFF",
                    "title": {
                        "type": "text",
                        "args": {
                            "text": "one listing",
                            "textScaleFactor": 1,
                            "style": "${getTextStyle('context','headline4',{'fontWeight':'normal','fontSize':30})}"
                        }
                    }
                }
            },
            "body": {
                "type": "set_scroll_controller",
                "args": {
                    "key": "controller"
                },
                "child": {
                    "type": "scrollbar",
                    "args": {
                        "controller": "${controller}"
                    },
                    "child": {
                        "type": "padding",
                        "args": {
                            "padding": {
                                "left": 5,
                                "right": 5
                            }
                        },
                        "child": {
                            "type": "custom_scroll_view",
                            "args": {
                                "controller": "${controller}",
                                "physics": {
                                    "type": "bouncing"
                                  }
                            },
                            "children": [
                                {
                                    "type": "sliver_grid",
                                    "args": {
                                        "gridDelegate": {
                                            "crossAxisCount": 1,
                                            "type": "fixed_cross_axis_count",
                                            "childAspectRatio": 2.5,
                                            "crossAxisSpacing": 10.0,
                                            "mainAxisSpacing": 20.0,
                                            "maxCrossAxisExtent": 500
                                        }
                                    },
                                    "children": "${for_in(entries['values'], 'template')}"
                                }
                            ]
                        }
                    }
                }
            }
        }
    }
}
}