{
  "type": "set_value",
  "args": {
    "errorImage": "assets/images/img_image_error.png",
    "imageError": {
      "type": "container",
      "args": {
        "width": 48,
        "height": 48,
        "alignment": "center"
      },
      "child": {
        "type": "asset_image",
        "args": {
          "fit": "contain",
          "name": "${errorImage}"
        }
      }
    },
    "template": {
      "type": "card",
      "args": {
        "elevation": 3,
        "margin": [12, 6, 12, 6],
        "shadowColor": "#20000000",
        "shape": {
          "type": "rounded",
          "borderRadius": {
            "type": "circular",
            "radius": 16
          }
        }
      },
      "child": {
        "type": "column",
        "args": {
          "mainAxisSize": "min",
          "mainAxisAlignment": "start",
          "crossAxisAlignment": "start"
        },
        "children": [
          {
            "type": "container",
            "args": {
              "height": 144,
              "decoration": {
                "borderRadius": {
                  "type": "only",
                  "topLeft": {
                    "type": "circular",
                    "radius": 16
                  },
                  "topRight": {
                    "type": "circular",
                    "radius": 16
                  }
                }
              }
            },
            "child": {
              "type": "stack",
              "args": {
                "alignment": "bottomLeft"
              },
              "children": [
                {
                  "type": "align",
                  "args": {
                    "alignment": "center"
                  },
                  "child": {
                    "type": "clip_rrect",
                    "args": {
                      "borderRadius": {
                        "type": "only",
                        "topLeft": {
                          "type": "circular",
                          "radius": 16
                        },
                        "topRight": {
                          "type": "circular",
                          "radius": 16
                        }
                      },
                      "clipBehavior": "antiAlias"
                    },
                    "child": {
                      "type": "network_image",
                      "args": {
                        "src": "${getImageUrl(value['img'])}",
                        "fit": "cover",
                        "width": "infinity",
                        "errorBuilder": "${imageErrorBuilder}"
                      }
                    }
                  }
                },
                {
                  "type": "container",
                  "args": {
                    "width": "infinity",
                    "padding": [12, 8, 12, 8],
                    "decoration": {
                      "gradient": {
                        "type": "linear",
                        "begin": "bottomCenter",
                        "end": "topCenter",
                        "colors": ["#80000000", "#00000000"]
                      }
                    }
                  },
                  "child": {
                    "type": "column",
                    "args": {
                      "mainAxisSize": "min",
                      "mainAxisAlignment": "center",
                      "crossAxisAlignment": "start"
                    },
                    "children": [
                      {
                        "type": "nullable_text",
                        "args": {
                          "text": "${value['title']}",
                          "maxLines": 1,
                          "overflow": "ellipsis",
                          "softWrap": true,
                          "style": {
                            "color": "#ffffffff",
                            "fontSize": 18,
                            "fontWeight": "w800"
                          }
                        }
                      },
                      {
                        "type": "nullable_text",
                        "args": {
                          "text": "${value['info2']}",
                          "maxLines": 2,
                          "overflow": "ellipsis",
                          "softWrap": true,
                          "style": {
                            "color": "#eeffffff",
                            "fontSize": 13,
                            "fontWeight": "w500"
                          }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "type": "padding",
            "args": {
              "padding": 12
            },
            "child": {
              "type": "row",
              "args": {
                "mainAxisSize": "max",
                "mainAxisAlignment": "start",
                "crossAxisAlignment": "center"
              },
              "children": [
                {
                  "type": "expanded",
                  "args": {
                    "flex": 7
                  },
                  "child": {
                    "type": "column",
                    "args": {
                      "mainAxisAlignment": "start",
                      "crossAxisAlignment": "start"
                    },
                    "children": [
                      {
                        "type": "nullable_text",
                        "args": {
                          "text": "${value['subtitle']}",
                          "maxLines": 2,
                          "overflow": "ellipsis",
                          "style": {
                            "color": "#ff212121",
                            "fontSize": 16,
                            "fontWeight": "w600"
                          }
                        }
                      },
                      {
                        "type": "nullable_text",
                        "args": {
                          "text": "${value['description']}",
                          "maxLines": 2,
                          "overflow": "ellipsis",
                          "style": {
                            "color": "#ff616161",
                            "fontSize": 14,
                            "fontWeight": "w600"
                          }
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "expanded",
                  "args": {
                    "flex": 3
                  },
                  "child": {
                    "type": "align",
                    "args": {
                      "alignment": "centerRight"
                    },
                    "child": {
                      "type": "nullable_text",
                      "args": {
                        "text": "${value['info']}",
                        "maxLines": 2,
                        "overflow": "ellipsis",
                        "style": {
                          "color": "#ff616161",
                          "fontSize": 12,
                          "fontWeight": "w600"
                        }
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  },
  "child": {
    "type": "scaffold",
    "args": {
      "appBar": {
        "type": "app_bar",
        "args": {
          "title": {
            "type": "row",
            "args": {
              "crossAxisAlignment": "start",
              "mainAxisAlignment": "start",
              "mainAxisSize": "min"
            },
            "children": [
              {
                "type": "text",
                "args": {
                  "text": "List"
                }
              }
            ]
          }
        }
      },
      "body": {
        "type": "list_view",
        "id": "data",
        "listen": ["data"],
        "args": {
          "physics": {
            "type": "bouncing"
          },
          "padding": [0, 12, 0, 12]
        },
        "children": "${for_each(data, 'template')}"
      }
    }
  }
}